<link rel="stylesheet" href="../public/css/style.css" />
<body>
  <!-- <%- include ('./bloks/header.ejs') %> -->
  <h1>Uploaded files list</h1>
  <form action="/api/upload/image/" method="post" enctype="multipart/form-data">
    <label for="photo">Upload image</label>
    <input type="file" id="photo" name="photo" required />
    <button type="submit">Upload</button>
  </form>
  <form action="/api/upload/sound/" method="post" enctype="multipart/form-data">
    <label for="sound">Upload sound</label>
    <input type="file" id="sound" name="sound" required />
    <button type="submit">Upload</button>
  </form>
  <div>
    <h1>Images</h1>
    <% imageList.forEach(item => { %>
    <div class="user_item">
      <div class="user_list">
        <%=item%>
        <img
          src="../public/images/<%=item%>"
          onerror="this.onerror=null;this.src='https://iconsplace.com/wp-content/uploads/_icons/800000/256/png/error-icon-8-256.png';"
          alt="image"
        />
      </div>
    </div>
    <% }) %>
  </div>

  <div>
    <h1>Sounds</h1>
    <% soundList.forEach(item => { %>
    <div class="user_item">
      <div class="user_list">
        <%=item%>
      </div>
    </div>
    <% }) %>
  </div>
</body>
